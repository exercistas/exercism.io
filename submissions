      AND c.user_id <> 28
      AND a.language = n.language AND a.slug = n.slug
      AND a.updated_at > (NOW()-interval '720 days')
      ORDER BY random(a.user_id)) AS exercises
ORDER BY nit_count ASC
LIMIT 5;
